    {
      "cell_type": "code",
      "source": [
        "forest_merged = reshape(forest_2010_ds, forest_2019_ds)\n",
        "forest_merged_hat = cluster_Agglomerative(forest_merged)[0]\n",
        "forest_merged_clusters = cluster_Agglomerative(forest_merged)[1]\n",
        "plot_clusters(\n",
        "        gas_merged_clusters, \n",
        "        gas_merged_hat, \n",
